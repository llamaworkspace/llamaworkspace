{
  "name": "llamaworkspace-app",
  "version": "0.0.12",
  "scripts": {
    "build": "next build",
    "dev": "next dev --turbo",
    "start-demo": "docker-compose -f ./infra/docker-compose.demo.yml up",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "pnpm run prisma:generate",
    "prisma:generate": "prisma generate",
    "lint": "next lint",
    "start": "next start",
    "prisma:dev": "pnpm run prisma migrate dev --name",
    "prisma:push": "pnpm run prisma db push",
    "prisma:deploy": "prisma migrate deploy",
    "db:deploy": "pnpm run prisma:deploy 2>&1 && tsx src/server/dataMigrations/dataMigrator.ts up 2>&1",
    "reset": "prisma migrate reset --force && prisma db push",
    "production:deploy": "pnpm run prisma:generate && pnpm run build && pnpm run db:deploy",
    "test:prepare": "env-cmd -f .env.test prisma migrate reset --force && env-cmd -f .env.test prisma db push",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "prepare": "husky install",
    "prettier": "prettier --config prettier.config.mjs --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "cloak:generate": "npx cloak generate | grep 'CLOAK_MASTER_KEY' | cut -d '=' -f2",
    "datamigrate": "env-cmd -f .env tsx src/server/dataMigrations/dataMigrator.ts",
    "datamigrate:create": "env-cmd -f .env tsx src/server/dataMigrations/dataMigrator.ts create --name",
    "datamigrate:up": "env-cmd -f .env tsx src/server/dataMigrations/dataMigrator.ts up",
    "datamigrate:down": "env-cmd -f .env tsx src/server/dataMigrations/dataMigrator.ts down",
    "dev:delete-all-openai-vector-stores": "env-cmd -f .env tsx src/scripts/dev_deleteAllOpenaiVectorStores.ts",
    "dev:delete-all-openai-files": "env-cmd -f .env tsx src/scripts/dev_deleteAllOpenaiFiles.ts",
    "dev:clear-openai": "pnpm run dev:delete-all-openai-vector-stores && pnpm run dev:delete-all-openai-files",
    "dco:up": "docker-compose -f docker-compose.yml up -d",
    "docker:build-local": "docker buildx build -t llamaworkspace/llamaworkspace:local -f ./infra/Dockerfile.demo ."
  },
