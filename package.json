{
  "name": "llamaws",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "check": "next lint && tsc --noEmit",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "postinstall": "prisma generate",
    "typecheck": "tsc --noEmit",
    "db:dev": "npx prisma migrate dev --name",
    "db:push": "npx prisma db push",
    "db:deploy": "prisma migrate deploy",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\" --cache",
    "dco:up": "docker-compose -f docker/docker-compose.yml up -d",
    "bump-deps": "npx npm-check-updates --deep -u && pnpm install",
    "hatchet:dev": "env-cmd -f .env -f .env.local npx tsx --watch src/hatchet/worker.ts",
    "hatchet:generate-token": "docker compose -f docker/docker-compose.yml exec hatchet-lite /hatchet-admin token create --config /config --tenant-id 707d0855-80ab-4e1f-a156-f1c4546cbf52 | xargs",
    "prepare": "husky install"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.7.2",
    "@hatchet-dev/typescript-sdk": "^0.15.0",
    "@prisma/client": "^5.14.0",
    "@radix-ui/react-alert-dialog": "^1.1.2",
    "@radix-ui/react-checkbox": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-icons": "^1.3.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.2.1",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.1",
    "@radix-ui/react-tooltip": "^1.1.4",
    "@t3-oss/env-nextjs": "^0.10.1",
    "@tanstack/react-query": "^5.50.0",
    "@trpc/client": "^11.0.0-rc.446",
    "@trpc/react-query": "^11.0.0-rc.446",
    "@trpc/server": "^11.0.0-rc.446",
    "ai": "^3.4.33",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "eslint-config-prettier": "^9.1.0",
    "geist": "^1.3.0",
    "lucide-react": "^0.456.0",
    "next": "^15.0.3",
    "next-auth": "5.0.0-beta.25",
    "nodemailer": "^6.9.16",
    "prettier-plugin-organize-imports": "^4.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-final-form": "^6.5.9",
    "server-only": "^0.0.1",
    "superjson": "^2.2.1",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "underscore": "^1.13.7",
    "zod": "^3.23.3"
  },
  "devDependencies": {
    "@types/eslint": "^8.56.10",
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/underscore": "^1.13.0",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "@typescript-eslint/parser": "^8.1.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.0.3",
    "husky": "^9.1.6",
    "postcss": "^8.4.39",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "prisma": "^5.14.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.5.3"
  },
  "packageManager": "pnpm@9.11.0",
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,jsx,ts,tsx,mjs,cjs}": "prettier --write"
  }
}
